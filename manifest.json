{
    "version": "1.0.0",
    "manifest_version": 3,
    "name": "Lazy Reader",
    "description": "This is a Chrome extension built with React and TypeScript. Some random description here",
    "icons": {
      "16": "icons/playful-ui/sloth/sloth16.png",
      "32": "icons/playful-ui/sloth/sloth32.png",
      "48": "icons/playful-ui/sloth/sloth48.png",
      "60": "icons/playful-ui/sloth/sloth60.png",
      "128": "icons/playful-ui/sloth/sloth128.png"
    },
    "action": {
      "default_popup": "js/popup.html",
      "default_title": "Lazy Reader",
      "default_sidebar": {
        "default_panel": "js/sidepanel.html",
        "open_at_install": true,
        "open_at_update": true
      }
    },
    "permissions": [
      "sidePanel", "activeTab","contextMenus","storage"
    ],
    "side_panel": {
      "default_path": "js/sidepanel.html"
    },
    "options_page": "js/options.html",
    "background": {
      "service_worker": "js/background.js"
    }
  }